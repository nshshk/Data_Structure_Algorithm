digraph single_linked_list {
        rankdir=LR;
        node [shape=record];
        head [label="{ <prev> | doubly_head | <next>  }"]
        a [label="{ <prev> | 3 | <next>  }"]
        //null [shape=none, label="null"];
        //null2 [shape=none, label="null"];
        head:next -> a [arrowhead=vee, tailclip=false];
}

