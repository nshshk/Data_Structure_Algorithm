digraph queue {
        rankdir=LR;
        node [shape=record];
        head [label="{ queue_head | <next>  }"]
        tail [label="{ queue_tail | <next>  }"]
        null [shape=none, label="null"];
        null2 [shape=none, label="null"];
        head -> null   [arrowtail=vee,  tailclip=false];
        tail -> null2   [arrowtail=vee,  tailclip=false];
}

